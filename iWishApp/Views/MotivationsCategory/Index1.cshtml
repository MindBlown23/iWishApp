@model List<iWishApp.Models.MotivationsCategory>

<h1>All Event Categories</h1>

@if(Model.Count == 0)
{
    <p>No Motivation Categories yet!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Id
             <th> Category Title</th>
        </tr>
        @foreach (MotivationsCategory category in Model)
        {
            <tr>
                <td>@category.Id</td>  
                <td>@category.Title</td>
            </tr>
        }
    </table>
}